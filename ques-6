#include<iostream>
using namespace std;



int main(){
    cout<<"The first element of the sequence "<<endl;
    int first;
    cin>>first;
    cout<<"first="<<first<<endl;
    cout<<"Constant difference between the elements "<<endl;
    int diff;
    cin>>diff;
    cout<<"diff="<<diff<<endl;
    cout<<"Total numbers in the sequence "<<endl;
    int n;
    cin>>n;
    cout<<"Total elements="<<n<<endl;

    int ele=first;
    cout<<'"';


    for(int i=0; i<n; i++){
        if(i!=n-1){
            cout<<ele<<","<<" ";
            ele=ele+diff;
        }
        else{
            cout<<ele<<'"';
        }
    }

    return 0;
}
